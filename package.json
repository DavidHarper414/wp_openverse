{
  "name": "openverse",
  "description": "Openverse is a search engine for openly-licensed content.",
  "private": true,
  "version": "0.0.0",
  "repository": "WordPress/openverse",
  "bugs": {
    "url": "https://github.com/WordPress/openverse/issues"
  },
  "author": "Openverse <openverse@wordpress.org>",
  "scripts": {
    "preeslint": "pnpm --filter eslint-plugin run build",
    "eslint": "eslint --ext .js,.ts,.vue,.json,.json5 --ignore-path .gitignore --ignore-path .eslintignore --max-warnings=0 frontend automations/js packages .pnpmfile.cjs .eslintrc.js tsconfig.json prettier.config.js"
  },
  "packageManager": "pnpm@7.17.1",
  "engines": {
    "node": ">= 18.0.0 <19",
    "pnpm": ">= 7.17.1"
  },
  "volta": {
    "node": "18.16.0"
  },
  "devDependencies": {
    "@babel/cli": "7.22.5",
    "@babel/core": "7.22.5",
    "@babel/eslint-parser": "7.22.5",
    "@babel/plugin-transform-typescript": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@intlify/eslint-plugin-vue-i18n": "1.4.0",
    "@nuxt/types": "2.17.0",
    "@openverse/eslint-plugin": "workspace:*",
    "@types/eslint": "8.40.0",
    "@types/express-useragent": "1.0.2",
    "@types/jest": "26.0.22",
    "@types/module-alias": "2.0.1",
    "@types/node": "17.0.32",
    "@types/throttle-debounce": "5.0.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-custom-alias": "1.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "42.0.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-vuejs-accessibility": "1.1.1",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "0.2.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "vue-tsc": "1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "consola",
        "eslint"
      ],
      "allowedVersions": {
        "babel-core": "7.0.0-bridge.0"
      }
    }
  }
}
